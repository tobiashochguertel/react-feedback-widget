{
  "name": "@react-visual-feedback/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Feedback Server - manage feedback, export reports, and integrate with CI/CD pipelines",
  "type": "module",
  "bin": {
    "feedback-cli": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "bun run ./dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts",
    "prepublishOnly": "bun run build && bun run test",
    "postinstall": "node -e \"console.log('\\n\\x1b[36mâœ¨ Thanks for installing @react-visual-feedback/cli!\\x1b[0m\\n\\nRun \\x1b[33mfeedback-cli --help\\x1b[0m to get started.\\n')\" 2>/dev/null || true"
  },
