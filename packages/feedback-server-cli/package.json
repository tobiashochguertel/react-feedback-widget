{
  "name": "@react-visual-feedback/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Feedback Server",
  "type": "module",
  "bin": {
    "feedback-cli": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "bun run ./dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@feedback/api-types": "workspace:*",
    "commander": "^14.0.2",
    "chalk": "^5.3.0",
    "inquirer": "^13.2.0",
    "ora": "^9.0.0",
    "cli-table3": "^0.6.5",
    "conf": "^15.0.2",
    "keytar": "^7.9.0",
    "boxen": "^8.0.1",
    "figures": "^6.1.0",
    "date-fns": "^4.1.0",
    "zod": "^4.3.5",
    "yaml": "^2.8.2",
    "csv-stringify": "^6.6.0",
    "update-notifier": "^7.3.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/update-notifier": "^6.0.8",
    "typescript": "^5.9.3",
    "tsup": "^8.5.1",
    "vitest": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cli",
    "feedback",
    "feedback-server",
    "command-line"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
