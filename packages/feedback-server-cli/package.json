    "chalk": "^5.3.0",
    "inquirer": "^13.2.0",
    "ora": "^9.0.0",
    "cli-table3": "^0.6.5",
    "conf": "^15.0.2",
    "keytar": "^7.9.0",
    "boxen": "^8.0.1",
    "figures": "^6.1.0",
    "date-fns": "^4.1.0",
    "zod": "^4.3.5",
    "yaml": "^2.8.2",
