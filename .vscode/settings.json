{
  /*
   VsCode extension: `alexkrechik.cucumberautocomplete`
  */
  "cucumberautocomplete.steps": [
    "packages/react-visual-feedback/tests/bdd/steps/*.ts",
  ],
  "cucumberautocomplete.syncfeatures": "packages/react-visual-feedback/tests/bdd/features/*.feature",
  "cucumberautocomplete.strictGherkinCompletion": true,
  "cucumberautocomplete.strictGherkinValidation": true,
  "cucumberautocomplete.smartSnippets": false,
  "cucumberautocomplete.stepsInvariants": false,
